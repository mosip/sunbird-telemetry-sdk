{
  "$id": "http://mosip.io/telemetry/error",
  "type": "object",
  "required": ["eid", "ets", "sid", "appid", "edata"],
  "allOf": [
    {
      "$ref": "http://mosip.io/telemetry/common"
    },
    {
      "properties": {
        "eid": {
          "type": "string",
          "enum": ["ERROR"]
        },
        "edata": {
          "type": "object",
          "additionalProperties": false,
          "required": ["errorId", "errorMessage", "stacktrace"],
          "allOf": [ 
            {
              "properties": {
                "type": {
                  "type": "string"
                },
                "errorId": {
                  "type": "string"
                },
                "errorMessage": {
                  "type": "string"
                },
                "stacktrace": {
                  "type": "string"
                },
                "env": {
                  "type": "string",
                  "minLength": 1
                },
                "injiVersion": {
                  "type": "string",
                  "minLength": 1
                },
                "tuvaliVersion": {
                  "type": "string",
                  "minLength": 1
                },
                "buildNumber": {
                  "type": "string",
                  "minLength": 1
                },
                "preferredLanguage": {
                  "type": "string"
                },
                "brandName": {
                  "type": "string",
                  "minLength": 1
                },
                "modelName": {
                  "type": "string",
                  "minLength": 1
                },
                "osName": {
                  "type": "string",
                  "minLength": 1
                },
                "osVersion": {
                  "type": "string",
                  "minLength": 1
                },
                "osApiLevel": {
                  "type": "string",
                  "minLength": 1
                },
                "isHardwareKeystoreSupported": {
                  "type": "string",
                  "minLength": 1
                },
                "dateTime": {
                  "type": "string",
                  "minLength": 1
                },
                "zone": {
                  "type": "string",
                  "minLength": 1
                },
                "offset": {
                  "type": "number"
                }
              }
            }
          ]
        }
      }
    }
  ]
}
