    {
      "$ref": "http://mosip.io/telemetry/common"
    },
    {
      "properties": {
        "eid": {
          "type": "string",
          "enum": ["ERROR"]
        },
        "edata": {
          "type": "object",
          "additionalProperties": false,
          "required": ["errorId", "errorMessage", "stacktrace"],
          "allOf": [
            {
              "$ref": "http://mosip.io/telemetry/edata_common"
            },
            {
              "$ref": "http://mosip.io/telemetry/device_information"
            },
            {
              "$ref": "http://mosip.io/telemetry/tags"
            }, 
            {
              "properties": {
                "errorId": {
                  "type": "string"
                },
                "errorMessage": {
                  "type": "string"
                },
                "stacktrace": {
                  "type": "string"
                }
              }
            }
          ]
